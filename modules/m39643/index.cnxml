                    <m:mn>1</m:mn>
                    <m:mo>-</m:mo>
                    <m:mn>7</m:mn>
                    <m:mo>×</m:mo>
                    <m:mi>i</m:mi>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="right">
