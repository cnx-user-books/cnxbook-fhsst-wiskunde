<m:mtr><m:mtd columnalign="right"><m:mn>28</m:mn></m:mtd>
<m:mtd><m:mo>=</m:mo></m:mtd>
<m:mtd columnalign="left"><m:mn>23</m:mn><m:mo>+</m:mo><m:mn>19</m:mn><m:mo>–</m:mo><m:mi>n(Nando’s en Steers)</m:mi></m:mtd></m:mtr>
<m:mtr><m:mtd columnalign="right"><m:mi>n(Nando’s en Steers)</m:mi></m:mtd>
<m:mtd><m:mo>=</m:mo></m:mtd>
