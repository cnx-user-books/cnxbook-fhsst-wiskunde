                  <m:msup>
                    <m:mi>a</m:mi>
                    <m:mi>n</m:mi>
                  </m:msup>
                </m:mrow>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:msup>
                  <m:mi>a</m:mi>
                  <m:mrow>
                    <m:mi>m</m:mi>
                    <m:mo>-</m:mo>
                    <m:mi>n</m:mi>
