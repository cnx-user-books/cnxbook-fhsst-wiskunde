            </m:msup>
          </m:mfrac>
        </m:math>
      </item>
      <item id="uid20">
        <m:math overflow="scroll">
          <m:msup>
            <m:mrow>
              <m:mo>(</m:mo>
              <m:mfrac>
                <m:mn>2</m:mn>
                <m:mn>3</m:mn>
              </m:mfrac>
              <m:mo>)</m:mo>
            </m:mrow>
            <m:mrow>
              <m:mo>-</m:mo>
              <m:mn>3</m:mn>
            </m:mrow>
          </m:msup>
        </m:math>
      </item>
      <item id="uid21">
        <m:math overflow="scroll">
          <m:mfrac>
            <m:mi>m</m:mi>
            <m:msup>
              <m:mi>n</m:mi>
              <m:mrow>
                <m:mo>-</m:mo>
                <m:mn>4</m:mn>
              </m:mrow>
            </m:msup>
          </m:mfrac>
        </m:math>
      </item>
      <item id="uid22">
        <m:math overflow="scroll">
          <m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mrow>
                  <m:mo>-</m:mo>
                  <m:mn>3</m:mn>
                </m:mrow>
              </m:msup>
              <m:mo>·</m:mo>
              <m:msup>
                <m:mi>x</m:mi>
                <m:mn>4</m:mn>
              </m:msup>
            </m:mrow>
            <m:mrow>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mn>5</m:mn>
              </m:msup>
              <m:mo>·</m:mo>
              <m:msup>
                <m:mi>x</m:mi>
                <m:mrow>
                  <m:mo>-</m:mo>
                  <m:mn>2</m:mn>
                </m:mrow>
              </m:msup>
            </m:mrow>
          </m:mfrac>
        </m:math>
