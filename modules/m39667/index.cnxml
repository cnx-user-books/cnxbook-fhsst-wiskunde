              <image mime-type="image/png" src="../../media/hyperbola1.png" id="uid12479_onlineimage" width="488"><!-- NOTE: attribute width changes image size online (pixels). original width is 488. --></image>
              <image mime-type="application/postscript" for="pdf" src="../../media/hyperbola1.eps" id="uid12479_printimage"/>
            </media>
          </figure>
</item>
</list>
</solution>
</exercise>
