                      <m:mn>1</m:mn>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>a</m:mi>
                      <m:mi>x</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mspace width="1.em"/>
                  <m:mspace width="1.em"/>
                  <m:mspace width="1.em"/>
