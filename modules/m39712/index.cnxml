                          </m:mrow>
                        </m:mfrac>
                      </m:mtd>
                      <m:mtd>
                        <m:mo>=</m:mo>
                      </m:mtd>
                      <m:mtd columnalign="left">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>D</m:mi>
