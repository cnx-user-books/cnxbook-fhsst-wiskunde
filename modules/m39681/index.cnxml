                      </m:mtd>
                    </m:mtr>
<m:mtr><m:mtd/>
                    <m:mtd><m:mo stretchy="false">=</m:mo></m:mtd>
                    <m:mtd columnalign="left"><m:mn>28</m:mn>
                  </m:mtd>
                </m:mtr>
              </m:mtable>
        </m:math>
      </equation></item>
  </list>
