                            <m:mi>B</m:mi>
                            <m:mi>C</m:mi>
                          </m:mrow>
                        </m:mfrac>
                      </m:mtd>
                      <m:mtd>
                        <m:mo>=</m:mo>
                      </m:mtd>
                      <m:mtd columnalign="left">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>D</m:mi>
                            <m:mi>E</m:mi>
                          </m:mrow>
                          <m:mrow>
                            <m:mi>E</m:mi>
                            <m:mi>F</m:mi>
                          </m:mrow>
                        </m:mfrac>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd columnalign="right">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>A</m:mi>
                            <m:mi>B</m:mi>
                          </m:mrow>
                          <m:mrow>
                            <m:mi>A</m:mi>
                            <m:mi>C</m:mi>
                          </m:mrow>
                        </m:mfrac>
                      </m:mtd>
                      <m:mtd>
                        <m:mo>=</m:mo>
                      </m:mtd>
                      <m:mtd columnalign="left">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>D</m:mi>
                            <m:mi>E</m:mi>
                          </m:mrow>
                          <m:mrow>
                            <m:mi>D</m:mi>
                            <m:mi>F</m:mi>
                          </m:mrow>
                        </m:mfrac>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd columnalign="right">
                        <m:mfrac>
                          <m:mrow>
                            <m:mi>A</m:mi>
                            <m:mi>C</m:mi>
                          </m:mrow>
                          <m:mrow>
